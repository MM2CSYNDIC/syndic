<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans SYSTEM "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

    
    <bean id="welcomePage" class="org.springframework.web.servlet.view.JstlView">
		<property name="url">
			<value>/index.jsp</value>
	</property>
	</bean>
	<bean id="adminPage" class="org.springframework.web.servlet.view.JstlView">
		<property name="url">
			<value>WEB-INF/views/pages/admin.jsp</value>
		</property>
	</bean>
	<bean id="loginPage" class="org.springframework.web.servlet.view.JstlView">
		<property name="url">
			<value>WEB-INF/views/pages/login.jsp</value>
		</property>
	</bean>
	<bean id="addQuestionPage" class="org.springframework.web.servlet.view.JstlView">
		<property name="url">
			<value>WEB-INF/views/pages/addQuestion.jsp</value>
		</property>
	</bean>
	<bean id="addUserPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>ADDUSERPAGE</value>
		</property>
	</bean>
	<bean id="addEventPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>ADDEVENTPAGE</value>
		</property>
	</bean>
    <bean id="addCondoPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
        <property name="url">
            <value>ADDCONDOPAGE</value>
        </property>
    </bean>
	<bean id="addProviderPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>ADDPROVIDERPAGE</value>
		</property>
	</bean>
	<bean id="listEventPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>LISTEVENTPAGE</value>
		</property>
	</bean>
    <bean id="affectEventPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>AFFECTEVENTPAGE</value>
		</property>
	</bean>
	<bean id="listProviderPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>LISTPROVIDERPAGE</value>
		</property>
	</bean>
	<bean id="pollingPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>POLLINGPAGE</value>
		</property>
	</bean>
	<bean id="pollingResultPage" class="org.springframework.web.servlet.view.tiles3.TilesView">
		<property name="url">
			<value>POLLINGRESULTPAGE</value>
		</property>
	</bean>
</beans>